# LazyPy Example 2:
# Volume of a sphere

def main(r):
    # Data section
    PI = 3.14
    Vol = 0
    
    # Code section
    PI = 3.14
    Vol = 4.0 / 3.0 * PI * (r ** 3)  # Using ** for exponentiation in Python
    print(Vol)

if __name__ == "__main__":
    # Example radius
    r = 5  # You can set this to any value you like
    main(r)

